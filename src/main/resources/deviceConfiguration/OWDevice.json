{
  "sensorDevice": false,
  "defaultGroup": "SWITCH",
  "sanitise": {
    "STATE": {
      "replace": "([0-9\\.]+) ([^0-9]+)",
      "replaceBy": "$1 ($2)"
    },
    "__general__": {
      "addAttributesIfNotPresent": [
        {
          "key": "offStateName",
          "value": "PIO 0"
        }
      ]
    }
  }
}