{
  "sensorDevice": true,
  "defaultGroup": "WEATHER",
  "sanitise": {
    "deviceValues": {
      "temp_c": {
        "append": "°C",
        "extract": "double"
      },
      "wind_chill": {
        "append": "°C"
      },
      "wind_condition": {
        "replaceAll": [
          {
            "search": "Wind: ",
            "replaceBy": ""
          }
        ]
      }
    }
  }
}